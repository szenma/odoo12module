<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="trans" model="account.account.template">
            <field name="name">Transfer Account</field>
            <field name="code">11003002</field>
            <field name="user_type_id" ref="account.data_account_type_current_assets"/>
            <field name="reconcile" eval="True"/>
            <field name="group_id" ref="l10n_qa.account_group_2"/>
        </record>
        <!-- Chart template -->
        <record id="l10nsa_chart_template" model="account.chart.template">
            <field name="name">Qatar COA</field>
            <field name="currency_id" ref="base.QAR"/>
            <field name="spoken_languages" eval="'en_US;ar_SY'"/>
            <field name="bank_account_code_prefix">1100200</field>
            <field name="cash_account_code_prefix">1100100</field>
            <field name="transfer_account_code_prefix">11003002</field>
        </record>
        <record id="trans" model="account.account.template">
            <field name="chart_template_id" ref="l10nsa_chart_template"/>
        </record>
</odoo>
